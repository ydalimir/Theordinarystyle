{
  "name": "destack",
  "description": "Page builder based on Next.js, Grapes.js & Tailwind CSS with zero-config deployment to Vercel",
  "version": "0.10.4",
  "main": "./build/browser/index.js",
  "homepage": "https://destack-page.vercel.app",
  "keywords": [
    "grapesjs",
    "nextjs",
    "builder",
    "nocode",
    "landing page",
    "tailwind"
  ],
  "scripts": {
    "dev": "rollup -c -w",
    "build": "rollup -c",
    "prepublishOnly": "cp ../../README.md README.md && npm run build",
    "postpublish": "rm -rf README.md"
  },
  "license": "ISC",
  "dependencies": {
    "formidable": "^1.2.2",
    "grapesjs": "^0.16.45",
    "is-svg-path": "^1.0.2"
  },
  "devDependencies": {
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^18.0.0",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "@types/formidable": "^1.2.2",
    "@types/react": "^17.0.8",
    "autoprefixer": "^10.2.6",
    "esbuild": "^0.12.5",
    "rollup": "^2.45.2",
    "rollup-plugin-esbuild": "^4.2.3",
    "rollup-plugin-import-css": "^2.0.1",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-svg-import": "^1.5.1",
    "tailwindcss": "^2.1.2",
    "tsc": "^2.0.3",
    "typescript": "^4.3.2",
    "typescript-plugin-css-modules": "^3.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/LiveDuo/destack.git"
  }
}
